<div class="body">
	<div class="container">
		<p id="notice"></p>
		<div class="row">
			<div class="col-xs-9">
				<h2>Items</h2>
					<table>
						<thead>
							<tr>
								<th>id</th>
								<th>title</th>
								<th>price</th>
								<th colspan="3" ></th>
							</tr>
						</thead>
						<tbody>
				  			<% @items.each do |item| %>
				  				<tr>
				  					<td><%= item.id %></td>
				  					<td><%= item.title %></td>
				  					<td><%= item.price %></td>
				  				</tr>
				  			<% end %>
				  		</tbody>
				  	</table>
			</div>
		</div>
	</div>
</div>
